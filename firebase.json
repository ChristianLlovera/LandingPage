{
  "hosting": {
    "public": "src/client/public",
    "rewrites": [{
      "source": "**/**",
      "function": "server"
    }],
    "ignore": [
      ".vscode/**",
      ".firebase/**",
      ".firebaserc",
      "firebase.json",
      "node_modules/**",
      "**/node_modules/**",
      "**/public/**",
      "*.log"
    ]
  },
  "functions": {
    "source": ".",
    "ignore": [
      ".vscode/**",
      ".firebase/**",
      ".firebaserc",
      "firebase.json",
      "node_modules/**",
      "**/node_modules/**",
      "**/public/**",
      "*.log"
    ]
  }
}